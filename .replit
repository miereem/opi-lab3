[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ant Build System"

[[workflows.workflow]]
name = "Ant Build System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = """
ant help && echo -e \"
=== Build System Ready ===\" && echo \"Available commands: ant compile, ant build, ant test, ant clean, etc.\" && echo \"Run 'ant <target>' to execute specific targets\""""

[workflows.workflow.metadata]
outputType = "console"
